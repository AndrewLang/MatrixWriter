{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": true
    },
    "exclude": [
        "node_modules",
        "typings"
    ],
    "angularCompilerOptions": {

        "skipMetadataEmit": true
    }
}